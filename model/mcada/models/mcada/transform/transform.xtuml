    if ( "" != codegen_type )
      LOG::LogInfo( message:"tranforming built-in type with name " + s_dt.Name );
      adatype = adatype::populate( name:s_dt.Name, domain_types_package:empty_domain_types_package, visibility:"public", adaname:codegen_type );
    end if;
  end if;
end for;',
