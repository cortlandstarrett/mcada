INSERT INTO S_SYNC
	VALUES ("c4b2375c-cede-4754-be51-f5e63fae417c",
	"00000000-0000-0000-0000-000000000000",
	'transformPackage',
	'',
	'ep_pkg = param.ep_pkg;

// create package
create object instance genfile of genfile;
genfile.name = T::sub(format:"lr", s:ep_pkg.Name);
genfile.base_dir = "";
genfile.extension = "ads";
genfile.file_path = genfile::get_full_path(base_dir:genfile.base_dir, name:genfile.name, extension:genfile.extension);
create object instance adapackage of adapackage;
adapackage.name = T::sub(format:"r", s:ep_pkg.Name);
adapackage.description = ep_pkg.Descrip;
relate adapackage to genfile across R200;

// transform classes
select many o_objs related by ep_pkg->PE_PE[R8000]->O_OBJ[R8001];
for each o_obj in o_objs
  ::transformModelClass(o_obj:o_obj, adapackage:adapackage);
end for;

// transform components
select many c_cs related by ep_pkg->PE_PE[R8000]->C_C[R8001];
select many ref_c_cs related by ep_pkg->PE_PE[R8000]->CL_IC[R8001]->C_C[R4201];
all_c_cs = c_cs | ref_c_cs;
for each c_c in all_c_cs
  ::transformComponent(c_c:c_c);
end for;

// transform inner packages
select many inner_ep_pkgs related by ep_pkg->PE_PE[R8000]->EP_PKG[R8001];
for each inner_ep_pkg in inner_ep_pkgs
  ::transformPackage(ep_pkg:inner_ep_pkg);
end for;',
	"ba5eda7a-def5-0000-0000-000000000000",
	1,
	'',
	0,
	0);
